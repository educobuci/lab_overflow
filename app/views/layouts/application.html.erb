<!DOCTYPE html>
<html>
<head>
  <title>LabOverflow</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <div id="structure">
    <div id="flash">
      <%= flash[:notice] %>
      <%= flash[:alert] %>
    </div>
    <div id="header">
      <h1>LabOverflow</h1>
    </div>
    <div id="menu">
      <%= link_to 'Sign In', new_user_session_path %>
    </div>
    <div id="content">
      <%= yield %>
    </div>
  </div>
</body>
</html>
