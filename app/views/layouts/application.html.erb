<!DOCTYPE html>
<html>
<head>
  <title>LabOverflow</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <div id="structure">
    <div id="flash">
      <%= flash[:notice] %>
      <%= flash[:alert] %>
    </div>
    <div id="header">
      <a href="/"><h1>LabOverflow</h1></a>
    </div>
    <div id="menu">
      <%= render :partial => 'shared/user_menu' %>
    </div>
    <div id="content">
      <%= yield %>
    </div>
  </div>
</body>
</html>